{
  "name": "motion-pointer",
  "version": "1.0.5",
  "description": "Small custom mouse cursor CSS & JS library for fancy UX.",
  "keywords": [
    "custom cursor",
    "custom pointer",
    "cursor js",
    "cursor css",
    "fancy cursor",
    "motion cursor",
    "cursor"
  ],
  "main": "dist/index.js",
  "scripts": {
    "dev:scss": "node-sass ./src/scss/index.scss --source-map true -o -r ./dist/index.css --output-style compressed",
    "dev:js": "minify ./src/js/cursor.js > ./dist/index.js",
    "dev": "npm-run-all -p dev:*",
    "watch": "watch \"npm run dev\" ./src"
  },
  "repository": "https://github.com/SG-WebDev/motion-pointer",
  "author": "SG-WebDev",
  "license": "MIT",
  "devDependencies": {
    "minify": "^5.1.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "watch": "^1.0.2"
  }
}
